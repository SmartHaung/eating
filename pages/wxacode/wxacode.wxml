<view class="body">
  <view class="nav bc_white">
    <view class="{{selected?'red':'default'}}" bindtap="selected">叫号二维码</view>
    <view class="{{selected1?'red':'default'}}" bindtap="selected1">添加管理员</view>
  </view>
  <view class="{{selected?'show':'hidden'}}">
    <view class='container'>
      <image src='{{queue}}' mode="aspectFit"></image>
      <button size="mini" type="primary" bindtap='download' data-url='{{queue}}'>保存到本地 </button>
    </view>
  </view>
  <view class="{{selected1?'show':'hidden'}}">
    <view class='container'>
      <image src='{{business}}' mode="aspectFit"></image>
      <button size="mini" type="primary" bindtap='download' data-url='{{business}}'>保存到本地 </button>
    </view>
  </view>
</view>