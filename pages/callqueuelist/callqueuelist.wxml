<view class="flex-row tr" style="display:flex;">
  <view class="flex-view-item">
    快捷叫号：
  </view>
  <view class="flex-view-item">
    <input placeholder="请输入订单号" bindinput="orderInput" auto-focus></input>
  </view>
  <view class="flex-view-item">
    <button size="mini" type="primary">完成 </button>
  </view>
</view>
<view class="table">
  <view class="tr bg-w">
    <view class="th">订单号</view>
    <view class="th">操作</view>
  </view>
  <block wx:for="{{listData}}" wx:key="{{callqueueOrderid}}">
    <view class="tr bg-g" wx:if="{{index % 2 == 0}}">
      <view class="td">{{item.callqueueOrderid}}</view>
      <view class="td">
        <button size="mini" type="default">部分 </button>
        <button size="mini" type="primary">完成 </button>
        <button size="mini" type="warn">取消 </button>
      </view>
    </view>
    <view class="tr" wx:else>
      <view class="td">{{item.callqueueOrderid}}</view>
      <view class="td">
        <button size="mini" type="default">部分 </button>
        <button size="mini" type="primary">完成 </button>
        <button size="mini" type="warn">取消 </button>
      </view>
    </view>
  </block>
</view>